  <portType name="LsrcPortType">
    <operation name="sCreateSurvey" >
    <documentation>Creates (Import and activate) a Survey in LimeSurvey</documentation>
      <input name="sCreateSurveyRequest" message="tns:sCreateSurveyRequest" />
